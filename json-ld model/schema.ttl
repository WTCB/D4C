@prefix d4c: <https://raw.githubusercontent.com/WTCB/D4C/main/Model/schema.ttl#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

d4c:Object a rdfs:Class ;
  rdfs:label "Object"@en ;
  rdfs:label "Object"@nl .

d4c:Unit a rdfs:Class ;
  rdfs:label "Unit"@en ,
             "Eenheid"@nl .

d4c:Property a rdfs:Class ;
  rdfs:label "Property"@en ;
  rdfs:label "Eigenschap"@nl .

d4c:hasProperty a rdf:Property ;
  rdfs:label "has property"@en ,
             "heeft eighenschap"@nl ;
  rdfs:domain d4c:Object ,
              d4c:Property ;
  rdfs:range d4c:Property .  

d4c:hasUnit a rdf:Property ;
  rdfs:label "has unit"@en ,
             "heeft eenheid"@nl ;
  rdfs:domain d4c:Property ;
  rdfs:range d4c:Unit .

d4c:name rdf:type owl:DatatypeProperty ;
  rdfs:label "name"@en ,
             "naam"@nl ;
  rdfs:domain d4c:Object ,
              d4c:Property ,
              d4c:Unit ;
  rdfs:range xsd:string .

d4c:reference rdf:type owl:DatatypeProperty ;
      rdfs:domain d4c:Object ,
                  d4c:Property ;
      rdfs:range xsd:anyURI .
